using System;
class Program 
	

{
    static void Main()
    {
	//Tworzenie listy elementów
        int[] array = { 1, 2 ,3, 5,  6, 10 ,200 , 400 ,4000};
        int  znajdzElement = 200 ;

        int index = szukajKota (array, znajdzElement);
	//Petla if
        if (index != -1)
        {
            Console.WriteLine($"Pierwsze wystąpienie liczby wynosi {znajdzElement} w liście: {index}");
        }
        else
        {
            Console.WriteLine($"Liczba {znajdzElement} nie znajduję się w liście.");
        }
    }
	//Funckja zwracajaca (target)
     static int szukajKota (ArrayList list, int target)
    {
        return list.IndexOf(target);
    }
}
